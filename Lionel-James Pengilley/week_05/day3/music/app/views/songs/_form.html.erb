<%= form_for @song do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :album %>
  <%= f.text_field :album %>

  <%= f.label :length %>
  <%= f.number_field :length %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :video %>
  <%= f.url_field :video %>

  <%= f.label :artist_id %>
  <%= f.select :artist_id, Artist.pluck(:name, :id), :include_blank => true %>

  <%= f.submit %>
<% end %>
